{
  "name": "hello-pdf",
  "version": "1.0.0",
  "description": "Hello-PDF micro service",
  "scripts": {
    "start": "yarn build && ./bin/hello-pdf",
    "start:server": "yarn build && ./bin/hello-pdf-server",
    "start:worker": "yarn build && ./bin/hello-pdf-worker",
    "start:events-logger": "yarn build && ./bin/hello-pdf-events-logger",
    "test": "jest --forceExit --runInBand --logHeapUsage",
    "build": "tsc --build",
    "build:clean": "tsc --build --clean",
    "build:force": "tsc --build --force",
    "prepare": "husky install",
    "prepack": "yarn build"
  },
  "bin": {
    "hello-pdf": "./bin/hello-pdf",
    "hello-pdf-server": "./bin/hello-pdf-server",
    "hello-pdf-worker": "./bin/hello-pdf-worker"
  },
  "files": [
    "dist/**/*",
    "bin/*"
  ],
  "dependencies": {
    "@appsignal/express": "^1.0.25",
    "@appsignal/nodejs": "^2.3.1",
    "@bull-board/api": "^3.9.4",
    "@bull-board/express": "^3.9.4",
    "bull": "^4.5.1",
    "compression": "^1.7.4",
    "debug": "^4.3.3",
    "express": "^4.17.1",
    "puppeteer": "^13.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/bull": "^3.15.8",
    "@types/compression": "^1.7.2",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.11",
    "@types/tmp": "^0.2.3",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "pdf2json": "^2.0.0",
    "prettier": "^2.5.1",
    "raw-body": "^2.4.2",
    "supertest": "^6.2.2",
    "tmp": "^0.2.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
