{
  "name": "hello-pdf",
  "version": "1.0.0",
  "description": "Hello-PDF micro service",
  "scripts": {
    "start": "yarn start:server & yarn start:worker & yarn start:events-logger",
    "start:server": "ts-node -P tsconfig.json src/server",
    "start:worker": "ts-node -P tsconfig.json src/worker",
    "start:events-logger": "ts-node -P tsconfig.json src/eventsLogger",
    "test": "jest --forceExit",
    "test-e2e": "yarn test-e2e:sync && yarn test-e2e:async",
    "test-e2e:sync": "ts-node tests/sync.ts",
    "test-e2e:async": "ts-node tests/async.ts",
    "build": "tsc --build",
    "build:clean": "tsc --build --clean",
    "build:force": "tsc --build --force",
    "prepare": "husky install",
    "prepack": "yarn build"
  },
  "bin": {
    "hello-pdf": "./bin/hello-pdf",
    "hello-pdf-server": "./bin/hello-pdf-server",
    "hello-pdf-worker": "./bin/hello-pdf-worker"
  },
  "files": [
    "dist/**/*",
    "bin/*"
  ],
  "dependencies": {
    "@appsignal/express": "^1.0.25",
    "@appsignal/nodejs": "^2.3.1",
    "body-parser": "^1.19.1",
    "bull": "^4.5.1",
    "express": "^4.17.1",
    "puppeteer": "^13.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.1.1",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/multer": "^1.4.7",
    "@types/supertest": "^2.0.11",
    "express-print-routes": "^1.0.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "raw-body": "^2.4.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
}
