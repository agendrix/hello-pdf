    "@types/pdf-parse": "^1.1.0",
    "@types/puppeteer": "^5.4.0",
    "@types/request": "^2.48.5",
    "aws-sdk": "^2.785.0",
    "mocha": "^6.2.0",
    "pdf-parse": "^1.1.1",
    "prettier": "^2.1.2",
    "puppeteer": "^5.4.1",
    "request": "^2.88.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "chrome-aws-lambda": "^5.3.1",
    "puppeteer-core": "^5.4.1"
